        <link rel="stylesheet" href="/node/main.css">
    </head>
    <body>
        <div class="container">
            <div class="jumbotron text-center">
                <h1><span class="fa fa-lock"></span> Node Authentication</h1>
                <p>Login or Register with:</p>
                
                <a href="/node/login" class="btn btn-default"><span class="fa fa-user"></span> Local Login</a>
                <a href="/node/signup" class="btn btn-default"><span class="fa fa-user"></span> Local Signup</a>
                <a href="/node/auth/google" class="btn btn-danger"><i class="fab fa-google"></i> Google</a>
                <a href="/node/auth/discord" class="btn btn-primary"><i class="fab fa-discord"></i> Discord</a>
                <a href="/node/auth/github" class="btn btn-dark"><i class="fab fa-github"></i> GitHub</a>

                <!-- if the user exists, display a profile button -->
                <% if (user) { %>
                    <a href="/node/profile" class="btn btn-primary"><i class="fas fa-user-circle"></i> Profile</a>
                <% } %>
            </div>
        </div>
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
